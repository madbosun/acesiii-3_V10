             call pack_coeffs_oed(palphas, pixalphas, ppcoeffs,
     *                       pixpcoeffs, ncfps, npfps, m, n,
     *                       alpha_pack, nalpha, pcoeff_pack,
     *                       ncoeff, pccbeg, pccend,
     *                       indx_cc,
     *                       ccbeg_pack, ccend_pack,
     *                       npcoeffs)
