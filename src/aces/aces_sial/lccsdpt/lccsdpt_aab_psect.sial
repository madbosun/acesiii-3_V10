                  if icount == whichi
                     REQUEST             TSaiai(a1,i1,a,j2) a
                     tpppp(a,a1,j2,i1) = TSaiai(a1,i1,a,j2)
                     jcount = 0.0
                     DO j1
                        jcount = jcount + 1
                        if jcount == whichj
                           kcount = 0.0
                           DO k1
