                                 REQUEST              Vpiqj(b,k1,j2,j1) b
                                 t1pppp(j2,b,k1,j1) = Vpiqj(b,k1,j2,j1)

                                 iicount = 0.0
                                 DO ii
                                    iicount = iicount + 1
                                    if iicount >= istart
                                    if iicount <= iend
                                       tppps(a,a1,j2,ii) = 0.0
                                       execute stripi tpppp tppps
                                       tppp(a,a1,j2) = tppps(a,a1,j2,ii)

                                       jjcount = 0.0
                                       DO jj
                                          jjcount = jjcount + 1
                                          if jj >= ii # VFL 
                                          if jjcount >= jstart
                                          if jjcount <= jend
                                             t1ppps(j2,b,k1,jj) = 0.0
                                             execute stripi t1pppp t1ppps
                                             t1ppp(j2,b,k1) = t1ppps(j2,b,k1,jj)

