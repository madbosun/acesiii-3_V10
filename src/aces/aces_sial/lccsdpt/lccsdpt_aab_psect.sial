                        t1ppps(a1,b,k1,jj) = 0.0 
                        execute stripi       t1pppp t1ppps 
                        t1ppp(a1,b,k1)     = t1ppps(a1,b,k1,jj)  
                        t1ppps(a1,b,k1,ii) = t1ppp(a1,b,k1)  
#
# ------------------------------------------------------------------------------------------------ 
#
#
               IF a == a1 
               IF ii == jj 
#
                        xai(a,ii)      = tpppp(a,a1,b,k1)*t1ppps(a1,b,k1,ii)
                        xai(a,ii)     *= fact  
                        PUT sai(a,ii) += xai(a,ii) 
#
                endif 
                endif 
#
# ------------------------------------------------------------------------------------------------ 
#
                     endif
                     endif
                     endif
                  ENDDO jj
                     endif
                     endif
                  ENDDO ii
#
               endif
             ENDDO j1
               endif
             ENDDO i1
               endif
             ENDDO k1
#
# END PERM 1 
# ---------- 
#
# PERM X 
# ------ 
#
            kcount = 0.0
            DO k1
               kcount = kcount + 1
               if kcount == kstart
            icount = 0.0
            DO i1
               icount = icount + 1
               if icount == whichi
            jcount = 0.0
            DO j1
               jcount = jcount + 1
               if jcount == whichj

                  REQUEST              VSpipi(a,i1,a1,j1) a 
                  t1pppp(a,a1,j1,i1) = VSpipi(a,i1,a1,j1)  
#
                  iicount = 0.0
                  DO ii
                     iicount = iicount + 1
                     if iicount >= istart
                     if iicount <= iend

                        t1ppps(a,a1,j1,ii) = 0.0 
                        execute stripi t1pppp t1ppps 
                        t1ppp(a,a1,j1) = t1ppps(a,a1,j1,ii) 

                  jjcount = 0.0
                  DO jj
                     jjcount = jjcount + 1
                     if jj >= ii # VFL 
                     if jjcount >= jstart
                     if jjcount <= jend
#
# Set the numerical factor 
# ------------------------ 
#
                        if a < a1
                        if ii < jj  
                           efact = 4.0 
                        endif
                        endif
#
                        if a == a1
                        if ii < jj  
                           efact = 2.0 
                        endif
                        endif
#
                        if a < a1
                        if ii == jj  
                           efact = 2.0 
                        endif
                        endif
#
                        if a == a1
                        if ii == jj  
                           efact = 1.0 
                        endif
                        endif
# ------------------------ 
#
                       #taaaiii(a,a1,b,k1,ii,jj)  = xaaaiii(a,a1,b,k1,ii,jj)
                       #execute energy_tdenominator taaaiii(a,a1,b,k1,ii,jj)
                        tpppp(a,a1,b,k1)          = xaaaiii(a,a1,b,k1,ii,jj) 
#
                        t1pps(a,a1,jj) = 0.0 
                        execute stripi   t1ppp t1pps 
                        t1pp(a,a1)     = t1pps(a,a1,jj)  
                          
                        tpp(b,k1)      = tpppp(a,a1,b,k1)*t1pp(a,a1)
                        tpp(b,k1)     *= efact  
                        PUT zai(b,k1) += tpp(b,k1) 
#
                     endif
                     endif
                     endif
                  ENDDO jj
                     endif
                     endif
                  ENDDO ii
#
               endif
             ENDDO j1
               endif
             ENDDO i1
               endif
             ENDDO k1
