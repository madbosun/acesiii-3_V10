                       xai(a,jj)          *= fact   
                       xai(a,jj)          *= -1.0  
                       PUT sai(a,jj)      += xai(a,jj) 
#
                       t4ppps(a,b,k1,ii) = 0.0 
                       execute stripi t4pppp t4ppps 
                       t4ppp(a,b,k1) = t4ppps(a,b,k1,ii) 
                       t4ppps(a,b,k1,jj) = t4ppp(a,b,k1) 
#
