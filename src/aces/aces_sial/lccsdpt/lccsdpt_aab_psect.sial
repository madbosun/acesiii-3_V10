     PROC READ_BLOCKDATA
#    -------------------
#
#   Read BLOCKDATA file.

      CREATE l1a_old
      execute sip_barrier
      execute server_barrier
      execute list_to_blocks                VSpipi(p,i1,p1,i2)
      execute list_to_blocks                Vaaii
      execute list_to_blocks                Viaai
      execute list_to_blocks                Vaaai
      execute list_to_blocks                Vpiqj(p,i1,q,j2)

      execute list_to_blocks                t1a_old
      execute list_to_blocks                TSaiai
      execute list_to_blocks                T2aiai

      execute list_to_blocks                l1a_old
      execute list_to_blocks                LSiaia
      execute list_to_blocks                L2iaia

      execute server_barrier
      execute read_list_to_blocks
      execute server_barrier

#   Anitsymmetrize Vaaai 

      PARDO a, a1, a2, i1
            REQUEST                      Vaaai(a,a1,a2,i1) a
            REQUEST                      Vaaai(a2,a1,a,i1) a
            taaai(a,a1,a2,i1)          = Vaaai(a2,a1,a,i1)
            taaai(a,a1,a2,i1)         -= Vaaai(a,a1,a2,i1)
            taaai(a,a1,a2,i1)         *= -1.0
            PREPARE VSaaai(a,a1,a2,i1) = taaai(a,a1,a2,i1)
      ENDPARDO a, a1, a2, i1

#   Anitsymmetrize T2aiai 

      PARDO a, a1, i, i1
            REQUEST T2aiai(a,i,a1,i1) a
            REQUEST T2aiai(a,i1,a1,i) a
            tpipi(a,i,a1,i1)  = T2aiai(a,i,a1,i1)
            t1pipi(a,i,a1,i1) = T2aiai(a,i1,a1,i)
            tpipi(a,i,a1,i1) -= t1pipi(a,i,a1,i1)
            PREPARE TSaiai(a,i,a1,i1) = tpipi(a,i,a1,i1)
      ENDPARDO a, a1, i, i1

      PARDO a, a1, i, i1
            REQUEST LSiaia(i,a,i1,a1) a
            tpipi(a,i,a1,i1) = LSiaia(i,a,i1,a1)
            PREPARE LSaiai(a,i,a1,i1) = tpipi(a,i,a1,i1)
      ENDPARDO a, a1, i, i1

      PARDO a, a1, i, i1
            REQUEST L2iaia(i,a,i1,a1) a
            tpipi(a,i,a1,i1) = L2iaia(i,a,i1,a1)
            PREPARE L2aiai(a,i,a1,i1) = tpipi(a,i,a1,i1)
      ENDPARDO a, a1, i, i1

      PARDO a, i
            GET l1a_old(i,a)
            tpp(a,i) = l1a_old(i,a)
            PREPARE sl1a(a,i) = tpp(a,i)
      ENDPARDO a, i

      execute server_barrier 
#
     ENDPROC READ_BLOCKDATA
