                       xai(a1,jj)          *= fact   
                       PUT sai(a1,jj)      += xai(a1,jj) 
#
              ENDIF 
              ENDIF 
#
# ---------------------------------------------------------------------------------------------------- 
#
                     endif
                     endif
                     endif
                  ENDDO jj
                     endif
                     endif
                  ENDDO ii
