      PROC V4O3_1 
#     ----------- 
#
# ------------------------------------------------------------------------------ 
#
# TERM 1 
#
# ------------------------------------------------------------------------------ 
#
            DO a3 
               kcount = 0.0 
            DO k1 
               kcount = kcount + 1 
               if kcount == kstart 
               REQUEST VSaaai(a,a3,a2,k1) a 

               tpppp (a,a3,a2,k1) = VSaaai(a,a3,a2,k1)

               jcount = 0.0 
               DO j1 
                  jcount = jcount + 1 
                  if jcount == whichj  
                     icount = 0.0 
                     DO i1 
                        icount = icount + 1 
                        if icount == whichi 

                           REQUEST  TSaiai(a3,i1,a1,j1) a3 
                           jjcount = 0.0 

                           DO jj 
                              jjcount = jjcount + 1 
                              if jjcount >= jstart  
                              if jjcount <= jend 

                                 tppps(a3,i1,a1,jj) = 0.0 
                                 execute stripi       TSaiai tppps  
                                 t1ppp(a3,i1,a1)    = tppps(a3,i1,a1,jj) 
                                 tppp(a3,a1,i1)     = t1ppp(a3,i1,a1)  
                                 iicount            = 0.0 

                                 DO ii 
                                    iicount = iicount + 1 
                                    if iicount >= istart  
                                    if iicount <= iend  

                                       tpps(a3,a1,ii)    = 0.0 
                                       execute stripi      tppp tpps  
                                       tpp(a3,a1)        = tpps(a3,a1,ii) 
                                       tpppp(a,a1,a2,k1) = VSaaai(a,a3,a2,k1)*tpp(a3,a1)  

                                      #taaaiii(a,a1,a2,k1,ii,jj)  = tpppp(a,a1,a2,k1) 
                                      #Xaaaiii(a,a1,a2,k1,ii,jj) += taaaiii(a,a1,a2,k1,ii,jj)
                                       execute sum_64ss Xaaaiii tpppp 

                                    endif  
                                    endif  
                                 ENDDO ii 

                              endif  
                              endif  
                           ENDDO jj 
                        ENDIF 
                     ENDDO i1
                  ENDIF 
               ENDDO j1
               endif 
            ENDDO k1
            ENDDO a3 
#
# ------------------------------------------------------------------------------ 
#
      ENDPROC V4O3_1 
