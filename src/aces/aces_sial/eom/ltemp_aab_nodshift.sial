               icount = icount + 1
               if icount == whichi
               REQUEST              Vpiqj(a1,i1,b,k1) a1 
               REQUEST              Vpiqj(a,i1,b,k1) a 
               t3pppp(a1,b,k1,i1) = Vpiqj(a1,i1,b,k1)  
               t4pppp(a,b,k1,i1)  = Vpiqj(a,i1,b,k1) 
            jcount = 0.0
            DO j1
               jcount = jcount + 1
               if jcount == whichj

                  REQUEST              Vpiqj(a,j1,b,k1) a 
                  REQUEST              Vpiqj(a1,j1,b,k1) b 
                  t1pppp(a1,b,k1,j1) = Vpiqj(a1,j1,b,k1)  
                  t2pppp(a,b,k1,j1)  = Vpiqj(a,j1,b,k1) 
#
                  iicount = 0.0
                  DO ii
                     iicount = iicount + 1
                     if iicount >= istart
                     if iicount <= iend
                  jjcount = 0.0
                  DO jj
                     jjcount = jjcount + 1
                     if jj >= ii # VFL 
                     if jjcount >= jstart
                     if jjcount <= jend
#
                        tpppp(a,a1,b,k1)   = xaaaiii(a,a1,b,k1,ii,jj) 
