                        t2ppps(a1,b,k1,jj) = t2ppp(a1,b,k1) 
#
                        xai(a,jj)           = tpppp(a,a1,b,k1)*t2ppps(a1,b,k1,jj) 
                        xai(a,jj)          *= fact   
                        xai(a,jj)          *= -1.0  
                        PUT sai(a,jj)      += xai(a,jj) 
