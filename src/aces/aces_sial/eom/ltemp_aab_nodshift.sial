            DO b1 
               jcount = 0.0 
            DO j1 
               jcount = jcount + 1 
               if jcount == whichj  
               REQUEST Vaaai(b1,b,a1,j1) b 

               tpppp (b1,b,a1,j1) = Vaaai (b1,b,a1,j1)

               kcount = 0.0 
               DO k1 
                  kcount = kcount + 1 
                  if kcount == kstart   
                     icount = 0.0 
                     DO i1 
                        icount = icount + 1 
                        if icount == whichi 

                          #REQUEST              T2aiai(a,i1,b1,k1) a 
                           REQUEST              T2aiai(b1,k1,a,i1) a 
                           t1pppp(b1,a,k1,i1) = T2aiai(b1,k1,a,i1) 
                           jjcount              = 0.0 
                           DO jj 
                              jjcount = jjcount + 1 
                              if jjcount >= jstart  
                              if jjcount <= jend 
                                 tppps(b1,b,a1,jj) = 0.0 
                                 execute stripi Vaaai tppps 
                                 tppp(b1,b,a1) = tppps(b1,b,a1,jj) 
                                 t1ppp(b1,a1,b) = tppp(b1,b,a1) 

                                 iicount = 0.0 

                                 DO ii 
                                    iicount = iicount + 1 
                                    if ii <  jj # VFL 
                                    if iicount >= istart  
                                    if iicount <= iend  
                                       t1ppps(b1,a,k1,ii) = 0.0 
                                       execute stripi t1pppp t1ppps 
                                       t2ppp(b1,a,k1) = t1ppps(b1,a,k1,ii) 

                                       tpppp(a,a1,b,k1) = t1ppp(b1,a1,b)*t2ppp(b1,a,k1)   
                                      #taaaiii(a,a1,b,k1,ii,jj)  = tpppp(a,a1,b,k1) 
                                      #Xaaaiii(a,a1,b,k1,ii,jj) += taaaiii(a,a1,b,k1,ii,jj)
                                       execute sum_64ss Xaaaiii tpppp 

                                    endif  
                                    endif  
                                    endif  
                                 ENDDO ii 

                              endif  
                              endif  
                           ENDDO jj 
                        ENDIF 
                     ENDDO i1
                  ENDIF 
               ENDDO k1
               endif 
            ENDDO j1
            ENDDO b1 
