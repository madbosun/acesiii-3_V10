     PROC OSEG_TRIP1 
#    ---------------  
#
# ---------------------------------------------------------- 
# Virtual symetry combination one.                          
# ---------------------------------------------------------- 

      PARDO a2, a1, a, k1
         allocate XTaaaiii(a,a1,a2,k1,*,*)
         allocate XRaaaiii(a,a1,a2,k1,*,*)

         deallocate XTaaaiii(a,a1,a2,k1,*,*)
         deallocate XRaaaiii(a,a1,a2,k1,*,*)
      ENDPARDO a2, a1, a, k1

# Set up the parallelization over the three virtual segments  
# ----------------------------------------------------------  
#
      PARDO a1, a, b 
      where a  <= a1 
#
# Loop over the occupied combinations 
# ----------------------------------- 
#
      ncount = 0.0 
      DO NT 

         ncount = ncount + 1.0  
        #execute print_scalar ncount 

         whichi = 1.0 
         execute get_ijk whichi ncount  

         istart = 2.0 
         execute get_ijk istart ncount  

         iend   = 3.0 
         execute get_ijk iend ncount  

         whichj = 4.0 
         execute get_ijk whichj ncount  

         jstart = 5.0 
         execute get_ijk jstart ncount  

         jend   = 6.0 
         execute get_ijk jend ncount  

         kstart = 7.0 
         execute get_ijk kstart ncount  
         kend = kstart 

         if kstart <= 0.0  
            exit 
         endif  
         if ncount <= xtemp1  
         IF whichj >= whichi   
#
# Allocate the partial amplitude array 
# ------------------------------------ 
#
            icount = 0 
            DO ii  
               icount = icount + 1 
               if icount >= istart 
               if icount <= iend  
               jcount = 0 
               DO jj 
                  jcount = jcount + 1 
                  if jcount >= jstart 
                  if jcount <= jend  
                  kcount = 0 
                  DO k1 
                     kcount = kcount + 1 
                     if kcount == kstart 
                        allocate Xaaaiii(a,a1,b,k1,ii,jj) 
                        allocate Raaaiii(a,a1,b,k1,ii,jj) 
                     endif 
                  ENDDO k1 
                  endif 
                  endif 
               ENDDO jj  
               endif 
               endif 
            ENDDO ii  
#
# First do the expensive part involving 3-virtual integrals and of order VVVVooo
# ------------------------------------------------------------------------------ 
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_1    
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_2   
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_3  
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_4 
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_5 
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_6 
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_7 
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V4O3_8 
#                
# Second do the cheaper part involving 3-virtual integrals and of order VVVVooo           
# ------------------------------------------------------------------------------         
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_1
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_2
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_3
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_4
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_5
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_6
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_7
      EXECUTE  PRINT_SCALAR  ZERO
      CALL V3O4_8
#                    
# Put Right hand T3 into Raaaiii  
# ------------------------------------------------------------------------------
#
            kcount = 0.0
            DO k1
               kcount = kcount + 1
               if kcount == kstart
            icount = 0.0
            DO i1
               icount = icount + 1
               if icount == whichi
            jcount = 0.0
            DO j1
               jcount = jcount + 1
               if jcount == whichj
#
                  iicount = 0.0
                  DO ii
                     iicount = iicount + 1
                     if iicount >= istart
                     if iicount <= iend
                  jjcount = 0.0
                  DO jj
                     jjcount = jjcount + 1
                     if jj >= ii # VFL 
                     if jjcount >= jstart
                     if jjcount <= jend
#
                        execute energy_tdenominator Xaaaiii(a,a1,b,k1,ii,jj)
                        Raaaiii(a,a1,b,k1,ii,jj) = Xaaaiii(a,a1,b,k1,ii,jj) 
                        Xaaaiii(a,a1,b,k1,ii,jj) = 0.0  
#
                     endif
                     endif
                     endif
                  ENDDO jj
                     endif
                     endif
                  ENDDO ii
#
               endif
             ENDDO j1
               endif
             ENDDO i1
               endif
             ENDDO k1
#                                                                              
# Compute Left hand L3
# ------------------------------------------------------------------------------
#
# First do the expensive part involving 3-virtual integrals and of order VVVVooo
# ------------------------------------------------------------------------------ 
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      EXECUTE  PRINT_SCALAR  ZERO
      CALL L4O3_1    
      EXECUTE  PRINT_SCALAR  ZERO
      CALL L4O3_2   
      EXECUTE  PRINT_SCALAR  ZERO
      CALL L4O3_3  
      EXECUTE  PRINT_SCALAR  ZERO
      CALL L4O3_4 
