     PROC IGUESS_UHF  
# 
#    ------------------------------------------------------------------------ 
#
     PARDO a, a1, i, i1 
#
           GET VSpipi(a,i,a1,i1) 
#
           Taiai(a,i,a1,i1)   = VSpipi(a,i,a1,i1)
           execute energy_denominator Taiai 
           PUT T2old_aa(a,i,a1,i1) = Taiai(a,i,a1,i1)  
#
     ENDPARDO a, a1, i, i1  
#
     PARDO b, b1, j, j1 
